#!/bin/bash
# postinst script for jolicloud-daemon
#
# see: dh_installdeb(1)

set -e

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#


# Remove the depricated /etc/apt/preferences which could still exist from
# the previous version of jolicloud-apt-config

if [ -e "/etc/apt/preferences" ]; then
    rm -f /etc/apt/preferences
fi
